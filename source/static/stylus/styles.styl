@import 'utils/normalize'
@import 'nib'

// Import fonts
@import 'utils/fonts'
@import 'utils/ico'

// Import config
@import 'config/identity'
// General
.wancho
	width 94%
	max-width 1024px
	margin auto
body
	font-size 15px
	font-family t-0
	color c-0
	-webkit-font-smoothing antialiased
	-webkit-tap-highlight-color transparent
a
	color inherit
	&:focus
		outline none
.link
	&:hover
		text-decoration underline
		color c-1
.relative
	position relative
.left
	float left
.right
	float right
.envolve
	overflow hidden
.child-ib
	&>*
		display inline-block
		vertical-align top
.no-style
	line-height 1.4

.g-text
	p
		margin-bottom 0
		margin-top s-5
		line-height 1.2
	p:first-child
		margin-top 0

// Para Proyecto en Produccion
